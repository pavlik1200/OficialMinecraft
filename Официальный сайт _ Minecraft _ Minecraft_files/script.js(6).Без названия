$(document).ready(function() {
    $('.header__reg-login.js-gotoauth').on('click', function () {
        $('.header__reg-title').text('Авторизация');
    });

    $('#form-register-e-mail, #form-register-auth-pass, #form-register-confirm-pass').on('click', function () {
        $('#register-error').html('');
    });
});